{
  "name": "with-vue-nuxt",
  "packageManager": "pnpm@9.10.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "commit": "git cz",
    "eslint": "eslint . --fix"
  },
  "dependencies": {
    "lodash-unified": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@components-ui/components": "workspace:^",
    "@components-ui/eslint-config": "workspace:^",
    "@components-ui/hooks": "workspace:^",
    "@components-ui/themes": "workspace:^",
    "@components-ui/utils": "workspace:^",
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/parser": "^8.17.0",
    "cz-git": "^1.3.10",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "vue": "^3.4.21"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "lint-staged": {
    "packages/**/*.{vue,js,ts,jsx,tsx,md,json}": "eslint --config .eslintrc.json --fix"
  }
}